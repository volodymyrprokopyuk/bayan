{% macro rightHand(config) %}
\tempo "Con moto, energico"
\clef treble
\key f \major
\time 3/4
\relative {
  {{ config() }}
  \repeat segno 2 {
    \volta 1
    <f'=' a c>4->\f <c f a> <a c f> |
  }
}
{% endmacro %}

{% macro leftHand(config) %}
\clef bass
\key f \major
\relative {
  {{ config() }}
  f,=,8-\stBass r fM! r fM r |
}
{% endmacro %}
