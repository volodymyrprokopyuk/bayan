\key e \minor
\relative {
  {{ config() }}
  e=4-\stBass em! em |
  e=4 em! em | fis b,@7! b74 | b, b7 b7 | e em! em |
  g,=,4 gM! gM | d' d7! d7 | fis d@7! d7 | g, gM! gM |
  b=,4 b7! b7 | e em! em | b b7! b7 | c cM! cM |
  a=,4 am! am | b e@m! em | b b7! b7 | e em! em |
  e=4 em! em | b b7! b7 | e( b g | e'=2) r4 |
}
{% endmacro %}
