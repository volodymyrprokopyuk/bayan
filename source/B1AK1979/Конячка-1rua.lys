{% macro rightHand(config) %}
\tempo Moderato
\clef treble
\key d \minor
\time 2/4
\relative {
  {{ config() }}
  \repeat volta 2 {
    d''=''8\(\mf cis d e | f e d4\) | d8\( cis bes g | bes a g4\) |
    a='8( f-.) e( c-.) | <d f>8-. 8-. 4-- | a'8 f e c | d d d='4 |
  }
}
{% endmacro %}

{% macro leftHand(config) %}
\clef bass
\key d \minor
\relative {
  {{ config() }}
  d=4 dm! | d dm | g, gm! | g,@m!2 |
  d'=4 c | bes besM! | a g=, | d,+dm!2 |
}
{% endmacro %}
