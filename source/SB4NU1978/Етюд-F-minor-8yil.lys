{% macro rhFrag1(a) %}
  aes'=''16(-> f)-. c-. aes-. g'(-> e)-. c-. g-. |
  f'=''16(-> c)-. aes-. f='-.  aes-. c-. f-. {{ a }} | %
{% endmacro %}

{% macro rhFrag2() %}
  c='''16(-> aes)-. f-. c-. bes'(-> g)-. e-. c-. |
  aes'=''16(-> f)-. c-. aes='-. %
{% endmacro %}

{% macro rhFrag3(a) %}
  {{ a }} c,16 c g'8-> c,16 c |
  f='8\< f f f16\( \after 16\! g |
  aes='8 g16 aes bes\> aes g f |
  e='8 \after 16.\! c c\) c16 c |
  aes'='8-> c,16 c g'8-> c,16 c |
  f='8 f f g16\(\< aes |
  g='8 f16 \after 16\! ees d8 g16 g, |
  c='8 c c'\) c16\( des |
  c=''8 bes16 aes g8 aes16 bes |
  c=''8 c c\)\< f16\( ges |
  f=''8 ees16 des c8 f16 \after 16\! ees |
  des=''8 bes bes\) ees16\( des |
  c=''8 bes16 aes g8 c16 e, |
  f='8 f f\) f16\( des |
  c='8 bes16 aes g8 c16 e=' |
{% endmacro %}

